<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/pid"
        android:layout_toEndOf="@+id/pid"
        android:layout_marginStart="41dp"
        android:id="@+id/type" />


    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nom"
        android:layout_below="@+id/date"
        android:layout_alignStart="@+id/date"
        android:layout_marginTop="37dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prenom"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/type"
        android:layout_marginTop="125dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prix"
        android:layout_alignStart="@+id/prenom"
        android:layout_below="@+id/nom" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/taille"
        android:layout_alignBottom="@+id/type"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp"
        android:layout_below="@+id/pid" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/date"
        android:layout_alignBaseline="@+id/prenom"
        android:layout_alignBottom="@+id/prenom"
        android:layout_alignStart="@+id/pid" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pid"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/taille"
        android:layout_marginTop="41dp" />
</RelativeLayout>
